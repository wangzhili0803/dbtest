<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jerry.dbtest">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_LOGS" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name="com.jerry.dbtest.FYApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.tencent.bugly.beta.ui.BetaActivity"
            android:theme="@android:style/Theme.Translucent" />

        <provider
            android:name="android.support.v4.content.FileProvider"
            android:authorities="com.jerry.dbtest.fileProvider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/provider_paths" />
        </provider>

        <activity
            android:name="com.jerry.dbtest.Activities.AsyncTaskLoaderActivity"
            android:label="@string/app_name" />

        <provider
            android:name="com.jerry.dbtest.provider.HelloProvider"
            android:authorities="com.jerry.dbtest.provider.HelloProvider" />

        <activity
            android:name="com.jerry.dbtest.Activities.MainActivity"
            android:label="@string/title_activity_loader" />
        <activity
            android:name="com.jerry.dbtest.Activities.LoaderActivity"
            android:label="@string/title_activity_async_task_loader" />
        <activity
            android:name="com.jerry.dbtest.Activities.RequestActivity"
            android:label="@string/title_activity_request" />
        <activity
            android:name="com.jerry.dbtest.Activities.AsyncActivity"
            android:label="@string/title_activity_async" />
        <activity
            android:name="com.jerry.dbtest.Activities.SoapActivity"
            android:label="@string/title_activity_soap" />
        <activity
            android:name="com.jerry.dbtest.Activities.WebActivity"
            android:label="@string/title_activity_web" />
        <activity
            android:name="com.jerry.dbtest.Activities.AnniActivity"
            android:label="@string/title_activity_anni" />
        <activity
            android:name="com.jerry.dbtest.Activities.MenuActivity"
            android:label="@string/title_activity_menu" />
        <activity
            android:name="com.jerry.dbtest.Activities.SurfaceViewActivity"
            android:label="@string/title_activity_surface_view" />
        <activity
            android:name="com.jerry.dbtest.Activities.BitmapActivity"
            android:label="@string/title_activity_bitmap">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.jerry.dbtest.Activities.MainStartActivity" />
        <activity android:name="com.jerry.dbtest.Activities.UpgradeActivity"></activity>
    </application>

</manifest>